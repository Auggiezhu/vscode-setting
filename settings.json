{
    // ----------  Editor  ----------
    // Better privacy
    "telemetry.telemetryLevel": "off",
    // Disable startup interface
    "workbench.startupEditor": "none",
    // Theme
    "workbench.colorTheme": "One Dark Pro Darker",
    // Font
    "editor.fontSize": 12,
    "editor.fontFamily": "'Iosevka Comfy'",
    //Line height
    "editor.lineHeight": 1.3,
    // Do not highlight occurances
    "editor.occurrencesHighlight": false,
    "editor.selectionHighlight": false,
    // Disable minimap
    "editor.minimap.enabled": false,
    // Cursor width
    "editor.cursorWidth": 2,
    // Disable light bulb
    "editor.lightbulb.enabled": false,
    // Disable breakpoints
    "editor.glyphMargin": false,
    // Column indicator
    "editor.rulers": [80],
    // Disable folding
    "editor.folding": false,
    // Vertical scroll bar size
    "editor.scrollbar.verticalScrollbarSize": 10,

    // ----------  Python  ----------
    "python.defaultInterpreterPath": "/opt/homebrew/Caskroom/miniforge/base/bin/python",
    "python.linting.enabled": false,
    "jupyter.sendSelectionToInteractiveWindow": true,
    "interactiveWindow.collapseCellInputCode": "never",
    "jupyter.askForKernelRestart": false,
    "terminal.integrated.inheritEnv": false,
    "[python]": {"editor.autoClosingBrackets": "always"},

    // ----------  Vim  ----------
    // General
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    // Leader key
    "vim.leader": "<space>",
    // Normal mode
    "vim.normalModeKeyBindingsNonRecursive": [
        {
        "before": ["<leader>", "<leader>"],
        "commands": ["workbench.action.showCommands"]
        },
        {
        "before": ["<leader>", "f", "s"],
        "commands": ["workbench.action.files.save"]
        },
        {
        "before": ["<leader>", "f", "r"],
        "commands": ["workbench.action.openRecent"]
        },
        {
        "before": ["<leader>", "b", "b"],
        "commands": ["workbench.action.quickOpen"]
        },
        {
        "before": ["<leader>", "b", "d"],
        "commands": ["workbench.action.closeActiveEditor"]
        },
        {
        "before": ["<leader>", "w", "o"],
        "commands": ["workbench.action.closeEditorsInOtherGroups"]
        },
        {
        "before": ["<leader>", "t", "r"],
        "commands": ["revealFileInOS"]
        },
        {
        "before": ["<C-g>"],
        "commands": [":nohl"]
        },
        {
        "before": [";", "a"],
        "after": ["^"]
        },
        {
        "before": [";", "e"],
        "after": ["$"]
        },
        {
        "before": [",", "a"],
        "after": ["{"]
        },
        {
        "before": [",", "e"],
        "after": ["}"]
        },
        {
        "before": ["d", "i", "s"],
        "after": ["d", "i", "'"]
        },
        {
        "before": ["d", "i", "d"],
        "after": ["d", "i", "\""]
        },
        {
        "before": [",", ","],
        "after": ["v", "i", "p", "h"]
        },
        {
        "before": ["m"],
        "after": ["%"]
        },
        {
        "before": ["g", "o", "c"],
        "after": ["^", "v", "$", "h", "d"]
        },
        {
        "before": ["<tab>"],
        "after": [">", ">"]
        },
        {
        "before": ["<shift+tab>"],
        "after": ["<", "<"]
        },
        {
        "before": [",", "c"],
        "commands": ["bracketeer.removeBrackets"]
        },
        {
        "before": [",", "k"],
        "after": ["v", "i", "w", "S", ")"]
        },
        {
        "before": [",", "f"],
        "after": ["v", "i", "w", "S", "]"]
        },
        {
        "before": [",", "h"],
        "after": ["v", "i", "w", "S", "}"]
        }
    ],
    // Visual mode
    "vim.visualModeKeyBindingsNonRecursive": [
        {
        "before": ["<leader>", "<leader>"],
        "commands": ["workbench.action.showCommands"]
        },
        {
        "before": ["<C-g>"],
        "after": ["<Esc>"]
        },
        {
        "before": [";", "a"],
        "after": ["^"]
        },
        {
        "before": [";", "e"],
        "after": ["$", "h"]
        },
        {
        "before": [",", "a"],
        "after": ["{"]
        },
        {
        "before": [",", "e"],
        "after": ["}"]
        },
        {
        "before": ["i", "k"],
        "after": ["i", ")"]
        },
        {
        "before": ["i", "f"],
        "after": ["i", "]"]
        },
        {
        "before": ["i", "h"],
        "after": ["i", "}"]
        },
        {
        "before": ["i", "s"],
        "after": ["i", "'"]
        },
        {
        "before": ["i", "d"],
        "after": ["i", "\""]
        },
        {
        "before": ["m"],
        "after": ["%"]
        },
        {
        "before": ["<tab>"],
        "after": [">"]
        },
        {
        "before": ["<shift+tab>"],
        "after": ["<"]
        },
    ],
    
    // Insert mode
    "vim.insertModeKeyBindingsNonRecursive": [
        {
        "before": ["f", "d"],
        "after": ["<Esc>"]
        },
        {
        "before": ["<C-k>"],
        "after": ["<backspace>"]
        },
        {
        "before": ["k", "k"],
        "after": ["("]
        },
        {
        "before": [",", ","],
        "after": ["["]
        },
        {
        "before": ["h", "h"],
        "after": ["{"]
        },
        {
        "before": ["d","h"],
        "after": ["="]
        },
        {
        "before": ["j", "j"],
        "after": ["+"]
        },
        {
        "before": ["j", "i"],
        "after": ["-"]
        },
        {
        "before": ["c","j"],
        "after": ["*"]
        },
        {
        "before": ["u", "u"],
        "after": ["_"]
        },
        {
        "before": ["a","a"],
        "after": ["&"]
        },
        {
        "before": ["v", "v"],
        "after": ["#"]
        },
        {
        "before": ["w", "w"],
        "after": ["?"]
        },
        {
        "before": ["b", "b"],
        "after": ["~"]
        },
        {
        "before": ["f", "h"],
        "after": ["%"]
        },
        {
        "before": ["m", "j"],
        "after": ["$"]
        },
        {
        "before": ["x", "y"],
        "after": ["<"]
        },
        {
        "before": ["d", "y"],
        "after": [">"]
        },
        {
        "before": ["g", "t"],
        "after": ["!"]
        },
        {
        "before": ["z", "l"],
        "after": ["|"]
        },
        {
        "before": ["s", "j"],
        "after": ["^"]
        },
        {
        "before": ["y", "b"],
        "after": ["Â£"]
        },
        {
        "before": ["q", "q"],
        "after": ["@"]
        },
        {
        "before": [";", "f"],
        "after": ["5"]
        },
        {
        "before": [";", "i"],
        "after": ["6"]
        },
        {
        "before": [";", "s"],
        "after": ["7"]
        },
        {
        "before": [";", "e"],
        "after": ["8"]
        },
        {
        "before": [";", "n"],
        "after": ["9"]
        },
        {
        "before": [";", "t"],
        "after": ["0"]
        }
    ],

    "vim.handleKeys": {
        "<C-a>": false,
        "<C-e>": false,
        "<C-f>": true
    },
}