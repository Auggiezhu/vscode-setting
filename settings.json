{
    "workbench.panel.defaultLocation": "right",
    "workbench.settings.editor": "json",
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "vscode-icons",
    "workbench.statusBar.visible": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.activityBar.visible": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.showTabs": true,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#89d0f123",
        "editorCursor.foreground": "#CF5A65",
        "editor.findMatchBackground": "#fa380888",
        "editor.findMatchHighlightBackground": "#fa380888",
        "editor.findRangeHighlightBackground": "#f0f3f23d",
        "editor.rangeHighlightBackground": "#0c89f059",
        "editorSuggestWidget.background": "#325270",
        "editorSuggestWidget.selectedBackground": "#f03c0f52",
        "editorHoverWidget.background": "#325270",
        "editor.selectionBackground": "#b6bebe46",
        "editorGutter.modifiedBackground": "#2686c5",
        "editorGutter.addedBackground": "#1fa56d",
        "editorGutter.deletedBackground": "#c53e26"
      },

    "editor.fontFamily": "Menlo",
    "editor.fontSize": 12,
    "editor.fontWeight": "500",
    "editor.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "editor.renderControlCharacters": false,
    "editor.highlightActiveIndentGuide": true,
    "editor.occurrencesHighlight": false,
    "editor.renderLineHighlight": "all",
    "editor.autoIndent": "full",
    "editor.wordWrap": "wordWrapColumn",
    "editor.lineHeight": 21,
    "editor.rulers": [80],
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "smooth",
