{
    "workbench.panel.defaultLocation": "right",
    "workbench.settings.editor": "json",
    "workbench.colorTheme": "Nord",
    "workbench.iconTheme": "vscode-icons",
    "workbench.statusBar.visible": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.activityBar.visible": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.showTabs": true,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,

    "editor.fontFamily": "Iosevka SS04",
    "editor.fontSize": 14,
    "editor.fontWeight": "500",
    "editor.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "editor.renderControlCharacters": false,
    "editor.highlightActiveIndentGuide": true,
    "editor.occurrencesHighlight": false,
    "editor.renderLineHighlight": "all",
    "editor.autoIndent": "full",
    "editor.wordWrap": "wordWrapColumn",
    "editor.lineHeight": 21,
    "editor.rulers": [80],
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "smooth",
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },

    "files.autoSave": "off",
    "files.trimTrailingWhitespace": true,

    "python.pythonPath": "/Users/ml/anaconda3/bin/python",
    "python.dataScience.jupyterServerURI": "local",
    "python.linting.enabled": false,
    "python.jediEnabled": false,
    "python.linting.lintOnSave": false,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "python.dataScience.askForKernelRestart": false,
    "python.dataScience.useNotebookEditor": true,
    "python.experiments.enabled": false,

    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,

    "search.location": "panel",
    "search.showLineNumbers": true,
    "search.followSymlinks": false,

    "terminal.integrated.fontSize": 14,
    "terminal.integrated.inheritEnv": true,

    "latex-workshop.view.pdf.viewer": "tab",

    "git.ignoreLegacyWarning": true,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.format": "${ago}",

    "macros": {
        "splitEmpty": [
          "workbench.action.splitEditor",
          "workbench.action.files.newUntitledFile",
          "workbench.action.closeEditorsToTheLeft"
        ],
        "selectionFind": [
            "actions.find",
            "toggleFindInSelection"
        ],
        "selectionReplace": [
            "editor.action.startFindReplaceAction",
            "toggleFindInSelection"
        ]
      },

    "vim.insertModeKeyBindings": [
        {
            "before": ["f","d"],
            "after": ["<Esc>"]
        },
        {
            "before": ["p","p"],
            "after": ["("]
        },
        {
            "before": ["s","s"],
            "after": ["["]
        },
        {
            "before": ["c","c"],
            "after": ["{"]
        },
        {
            "before": ["e","e"],
            "after": ["="]
        },
        {
            "before": ["j","j"],
            "after": ["+"]
        },
        {
            "before": ["m","m"],
            "after": ["-"]
        },
        {
            "before": ["t","t"],
            "after": ["*"]
        },
        {
            "before": ["h","h"],
            "after": ["#"]
        }
    ],
    "vim.leader": "<space>",
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["f","d"],
            "after": ["<Esc>"]
        },
        {
            "before": ["<leader>","<space>"],
            "commands": ["workbench.action.showCommands"]
        },
        {
            "before": ["<leader>","c","l"],
            "commands": ["editor.action.commentLine"]
        },
        {
            "before": ["<leader>","e","f"],
            "commands": ["editor.action.formatDocument"]
        },
        {
            "before":  ["<leader>","s","i"],
            "commands": ["macros.selectionFind"],
            "after": ["toggleFindRegex"]
        },
        {
            "before":  ["<leader>","s","r"],
            "commands": ["macros.selectionReplace"],
            "after": ["toggleFindRegex"]
        }
    ],

    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["g","i"],
            "after": ["g","i","<Esc>"]
        },
        {
            "before": ["<leader>","u","e"],
            "after": ["v","$","h"]
        },
        {
            "before": ["<leader>","o"],
            "after": ["o","<Esc>","k"]
        },
        {
            "before": ["<leader>","<space>"],
            "commands": ["workbench.action.showCommands"]
        },
        {
            "before": ["<leader>","c","l"],
            "commands": ["editor.action.commentLine"]
        },
        {
            "before": ["<leader>","n","n"],
            "commands": ["notifications.clearAll"]
        },
        {
            "before": ["<leader>","b","t"],
            "commands": ["workbench.action.openNextRecentlyUsedEditorInGroup"]
        },
        {
            "before": ["<leader>","b","b"],
            "commands": ["workbench.action.showAllEditors"]
        },
        {
            "before": ["<leader>","b","d"],
            "commands": ["workbench.action.closeActiveEditor"]
        },
        {
            "before": ["<leader>","b","h"],
            "commands": ["workbench.action.moveEditorToPreviousGroup"]
        },
        {
            "before": ["<leader>","b","l"],
            "commands": ["workbench.action.moveEditorToNextGroup"]
        },
        {
            "before": [ "<leader>","b","n"],
            "commands": ["workbench.action.nextEditor"]
        },
        {
            "before": [ "<leader>","b","p"],
            "commands": ["workbench.action.previousEditor"]
        },
        {
            "before": ["<leader>","b","r"],
            "commands": ["workbench.action.openRecent"]
        },
        {
            "before": ["<leader>","f","c"],
            "commands": ["fileutils.duplicateFile"]
        },
        {
            "before": ["<leader>","f","d"],
            "commands": ["workbench.action.openGlobalSettings"]
        },
        {
            "before": ["<leader>","f","f"],
            "commands": ["workbench.action.files.openFileFolder"]
        },
        {
            "before": ["<leader>","f","m"],
            "commands": ["fileutils.moveFile"]
        },
        {
            "before": ["<leader>","f","n"],
            "commands": ["workbench.action.files.newUntitledFile"]
        },
        {
            "before": ["<leader>","f","p"],
            "commands": ["workbench.action.quickOpen"]
        },
        {
            "before": ["<leader>","f","r"],
            "commands": ["fileutils.renameFile"]
        },
        {
            "before": ["<leader>","f","s"],
            "commands": ["workbench.action.files.save"]
        },
        {
            "before": ["<leader>","f","t"],
            "commands": ["workbench.view.explorer"]
        },
        {
            "before": ["<leader>","f","y"],
            "commands": ["workbench.action.files.copyPathOfActiveFile"]
        },
        {
            "before": ["<leader>","g","h"],
            "commands": ["gitlens.showQuickFileHistory"]
        },
        {
            "before": ["<leader>","g","f"],
            "commands": ["gitlens.toggleFileBlame"]
        },
        {
            "before": ["<leader>","g","l"],
            "commands": ["gitlens.toggleLineBlame"]
        },
        {
            "before": ["<leader>","1"],
            "commands": ["workbench.action.focusFirstEditorGroup"]
        },
        {
            "before": ["<leader>","2"],
            "commands": ["workbench.action.focusSecondEditorGroup"]
        },
        {
            "before": ["<leader>","3"],
            "commands": ["workbench.action.focusThirdEditorGroup"]
        },
        {
            "before": ["<leader>","4"],
            "commands": ["workbench.action.focusFourthEditorGroup"]
        },
        {
            "before": ["<leader>","w","d"],
            "commands": ["workbench.action.closeEditorsInGroup"]
        },
        {
            "before": ["<leader>","w","e"],
            "commands": ["macros.splitEmpty"]
        },
        {
            "before": [ "<leader>","w","h"],
            "commands": ["workbench.action.splitEditorDown"]
        },
        {
            "before": [ "<leader>","w","j"],
            "commands": ["workbench.action.joinTwoGroups"]
        },
        {
            "before": ["<leader>","w","v"],
            "commands": ["workbench.action.splitEditorRight"]
        },
        {
            "before":  ["<leader>","s","g"],
            "commands": ["workbench.action.findInFiles"]
        },
        {
            "before": ["<leader>","s","s"],
            "commands": ["actions.find"],
            "after": ["toggleFindRegex"]
        },
        {
            "before": ["<leader>","s","h"],
            "commands": ["editor.action.startFindReplaceAction"],
            "after": ["toggleFindRegex"]
        },
        {
            "before": ["leader","j","f"],
            "after": ["leader","leader","f"]
        },
        {
            "before": ["leader","j","j"],
            "after": ["leader","leader","s"]
        },
        {
            "before": ["leader","j","l"],
            "after": ["leader","leader","leader","b","d","j","k"]
        },
        {
            "before": ["<leader>","j","s"],
            "commands": ["workbench.action.gotoSymbol"]
        },
        {
            "before": ["leader","j","w"],
            "after": ["leader","leader","leader","b","d","w"]
        },
        {
            "before": ["<leader>","e","e"],
            "commands": ["editor.action.smartSelect.grow"]
        },
        {
            "before": ["<leader>","e","f"],
            "commands": ["editor.action.formatDocument"]
        },
        {
            "before": [ "<leader>","e","s"],
            "commands": ["editor.action.smartSelect.shrink"]
        },
        {
            "before": [ "<leader>","t","a"],
            "commands": ["workbench.action.toggleActivityBarVisibility"]
        },
        {
            "before": ["<leader>","t","e"],
            "commands": ["workbench.view.explorer"]
        },
        {
            "before": ["<leader>","t","g"],
            "commands": ["workbench.view.scm"]
        },
        {
            "before": ["<leader>","t","k"],
            "commands": ["workbench.action.openGlobalKeybindings"]
        },
        {
            "before": ["<leader>","t","p"],
            "commands": ["workbench.action.togglePanel"]
        },
        {
            "before": ["<leader>","t","s"],
            "commands": ["workbench.action.toggleSidebarVisibility"]
        },
        {
            "before": ["<leader>","t","t"],
            "commands": ["workbench.action.selectTheme"]
        },
        {
            "before": ["<leader>","t","x"],
            "commands": ["workbench.view.extensions"]
        },
        {
            "before": ["<leader>","t","z"],
            "commands": ["workbench.action.terminal.toggleTerminal"]
        },
        {
            "before": ["<leader>","q","w"],
            "after": ["y","s","i","w"]
        },
        {
            "before": ["<leader>","q","q"],
            "after": ["y","s","i","W"]
        },
        {
            "before": ["<leader>","a","s"],
            "commands": ["workbench.action.toggleScreencastMode"]
        }
    ],
    "vim.easymotion": true,
    "vim.easymotionMarkerHeight": 18,
    "vim.visualstar": true,
    "vim.surround": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.useCtrlKeys": false,
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-f>": true,
        "<C-u>": true,
        "<C-v>": true,
        "<C-b>": true
    },

    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false
}
